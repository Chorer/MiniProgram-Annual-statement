.enterNoneShell {
  position: relative;
  width: 100%;
  height: 100%;
}
.bg {
  position: absolute;
  z-index: -1;
  top:0;
  left:0;
  width: 100%;
  height: 100%;
}
.enterNoneShell .part-one {
  position: absolute;
  top:80rpx;
  width: 100%;
  height: 300rpx;
  margin-left: 30rpx;
  /* background-color: aqua; */
}
.enterNoneShell  .word {
  display: block;
  opacity: 0;
}
.enterNoneShell  .changeWord {
  color: var(--text-color);
  margin: 0 10rpx;
}
.enterNoneShell  .changeWordStyle {
  margin-left: 0;
}
.enterNoneShell  .part-two {
  position: absolute;
  top: 430rpx;
  width: 100%;
  height: 300rpx;
  text-align: end;
}
.enterNoneShell  .wordStyle {
  padding-right: 40rpx;
  color:var(--text-color);
}
.enterNoneShell .part-one>view {
  animation-delay: .8s;
}
.enterNoneShell .part-one>text {
  animation-delay: 1.6s;
}
.enterNoneShell .part-two .word:nth-of-type(1) {
  animation-delay: 2.4s;
}
.enterNoneShell .part-two .word:nth-of-type(2) {
  animation-delay: 2.4s;
}